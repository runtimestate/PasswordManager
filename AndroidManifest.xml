<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.life.android.pwmanager"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="7"
        android:targetSdkVersion="15" />

    <application
        android:icon="@drawable/ic_launcher"
        android:label="@string/app_name" >
        <activity
            android:name="com.life.android.pwmanager.activity.UserActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.life.android.pwmanager.activity.MessageActivity" >
        </activity> 
        <activity android:name="com.life.android.pwmanager.activity.MainActivity" >
        </activity>
        <activity android:name="com.life.android.pwmanager.activity.ResetPasswordActivity" >
        </activity>
        <activity android:name="com.life.android.pwmanager.activity.ItemActivity" >
        </activity>
        <activity android:name="com.life.android.pwmanager.activity.AboutActivity" >
        </activity>
    </application>

</manifest>